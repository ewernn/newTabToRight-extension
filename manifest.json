{
  "manifest_version": 3,
  "name": "New Tab Right",
  "version": "1.1",
  "commands": {
    "new-tab-right": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "description": "Open new tab to the right"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"]
}